# Signal.Engine Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# DATA PROVIDER
# =============================================================================
# Choose: "alpaca" or "yfinance"
DATA_PROVIDER=yfinance

# =============================================================================
# ALPACA CONFIGURATION (Required if DATA_PROVIDER=alpaca)
# =============================================================================
# Get your free API keys at: https://app.alpaca.markets/signup
# Paper trading is free and works great for testing!

ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here
ALPACA_PAPER=true

# =============================================================================
# APP SETTINGS
# =============================================================================
DEBUG=false
